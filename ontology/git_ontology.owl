<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/git-onto-logic.owl"
         xmlns="http://example.org/git-onto-logic.owl#">

<owl:Ontology rdf:about="http://example.org/git-onto-logic.owl"/>

<owl:ObjectProperty rdf:about="#hasBranch">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#Branch"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCommit">
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="#Commit"/>
  <owl:inverseOf rdf:resource="#isCommitOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isCommitOf">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Branch"/>
  <owl:inverseOf rdf:resource="#hasCommit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authoredBy">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#User"/>
  <owl:inverseOf rdf:resource="#authoredCommit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#authoredCommit">
  <rdfs:domain rdf:resource="#User"/>
  <rdfs:range rdf:resource="#Commit"/>
  <owl:inverseOf rdf:resource="#authoredBy"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#modifiesFile">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#File"/>
  <owl:inverseOf rdf:resource="#modifiedByCommit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#modifiedByCommit">
  <rdfs:domain rdf:resource="#File"/>
  <rdfs:range rdf:resource="#Commit"/>
  <owl:inverseOf rdf:resource="#modifiesFile"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasParent">
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="#Commit"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasFile">
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="#File"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#branchName">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#timestamp">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#commitMessage">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#isDefault">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Branch"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#parentCount">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Commit"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#repoName">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Repository"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Repository">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasBranch"/>
      <owl:onClass rdf:resource="#Branch"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasFile"/>
      <owl:onClass rdf:resource="#File"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Branch">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#branchName"/>
      <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCommit"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#User">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#File">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Commit">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#authoredBy"/>
      <owl:onClass rdf:resource="#User"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#timestamp"/>
      <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#commitMessage"/>
      <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#modifiesFile"/>
      <owl:someValuesFrom rdf:resource="#File"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#InitialCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasParent"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">0</owl:maxQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#RegularCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasParent"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#MergeCommit">
  <rdfs:subClassOf rdf:resource="#Commit"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasParent"/>
      <owl:onClass rdf:resource="#Commit"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>


</rdf:RDF>
